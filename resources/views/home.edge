    <title>Cleaning Garden - Home</title>
  </head>
    <body style="background: #F7F7F7">
      <nav class="navbar sticky-top navbar-expand-lg navbar-light bg-light">
        <div class="container-fluid">
          <a class="navbar-brand" href="/" style="color: #2E5238; font-family: 'Poppins', sans-serif; font-size: 25px">
            <img src="images/logo1.png" alt="" width="30" height="30" class="d-inline-block align-text-top">
              Cleaning Garden
          </a>
              <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
              </button>
              <div class="d-flex" id="navbarSupportedContent" style="margin-right: 40px; font-family: 'Poppins', sans-serif;">
                <ul class="navbar-nav mb-2 mb-lg-0">
                  <li class="nav-item">
                    <a class="nav-link"  href="/beranda">Beranda</a>
                  </li>
                  <li class="nav-item">
                    <a class="nav-link" href="/layanan">Layanan</a>
                  </li>
                  <li class="nav-item">
                    <a class="nav-link" href="/tentang">Tentang Kami</a>
                  </li>
                  @loggedIn
                  <li class="nav-item">
                    <a class="nav-link" href="/keluar">Keluar</a>
                  </li>
                  @else
                  <li class="nav-item">
                    <a class="nav-link" href="/masuk">Masuk/Daftar</a>
                  </li>
                  @endloggedIn
                </ul>
              </div> 
            </div>
          </nav>
    </nav>
    <div class="d-flex bd-highlight">
      <div class="p-2 bd-highlight">
        <picture>
          <source srcset="..." type="...">
          <img src="images/img-home.png" class="rounded d-block mb-4 mt-5" alt="..." style="height:560px; width: 560px; margin-left: 50px;">
        </picture>
      </div>
      <div class="align-self-center">
        <h1 style="color: #2E5238; font-family: 'Poppins', sans-serif; font-size: 60px;">Clean Your Garden</h1>
        <div class="align-self-end" style="margin-left: 124px;">
        <h5 style="color: #2E5238; font-family: 'Poppins', sans-serif; font-size: 20px;">Solusi kami untuk membersihkan taman mu</h5>
        @loggedIn  
        <a type="button" class="btn btn-success mt-3" style="width: 150px; margin-left: 294px;" href="/keluar">Keluar</a>
        @else
        <a type="button" class="btn btn-success mt-3" style="width: 150px; margin-left: 294px;" href="/daftar">Daftar</a>  
        @endloggedIn  
        </div>
      </div>
    </div>

    <!-- Optional JavaScript; choose one of the two! -->

